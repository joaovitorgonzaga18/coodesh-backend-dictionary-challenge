      "/entries/en": {
        "get": {
          "summary": "Search Entries",
          "parameters": [
            {
              "name": "search",
              "in": "query",
              "required": true,
              "schema": {
                "type": "string"
              }
            },
            {
              "name": "limit",
              "in": "query",
              "required": true,
              "schema": {
                "type": "integer"
              }
            },
            {
              "name": "current_page",
              "in": "query",
              "required": true,
              "schema": {
                "type": "integer"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Success",
              "content": {
                "application/json": {
                  "example": {
                    "results": [
                      "fire",
                      "fireable",
                      "firearm",
                      "firearmed"
                    ],
                    "totaldocs": 137,
                    "page": "1",
                    "totalPages": 34,
                    "hasNext": true,
                    "hasPrev": false
                  }
                }
              }
            },
            "400": {
              "description": "Bad Request",
              "content": {
                "application/json": {
                  "example": {
                    "message": "Error message"
                  }
                }
              }
            }
          }
        }
      },
